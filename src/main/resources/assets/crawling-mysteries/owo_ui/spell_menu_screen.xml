<owo-ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/wisp-forest/owo-lib/1.20/owo-ui.xsd">
  <components>
    <flow-layout direction="vertical">
      <children>

        <flow-layout direction="vertical">

          <children>
            <label id="tooltip">
              <text>i'm a tooltip</text>
            </label>
          </children>

          <padding>
            <all>5</all>
          </padding>

          <surface>
            <tooltip></tooltip>
          </surface>

          <positioning type="relative">10,50</positioning>

        </flow-layout>

        <flow-layout direction="vertical">
          <children>

            <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
              region-width="61" region-height="59" id="favorite">
            </texture>

            <!-- Favorites -->
            <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
              u="61" v="0" region-width="21" region-height="23" id="spell_favorite_1">
              <cursor-style>hand</cursor-style>
              <positioning type="absolute">20,4</positioning>
            </texture>

            <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
              u="60" v="0" region-width="21" region-height="23" id="spell_favorite_2">
              <cursor-style>hand</cursor-style>
              <positioning type="absolute">4,18</positioning>
            </texture>

            <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
              u="61" v="0" region-width="21" region-height="23" id="spell_favorite_3">
              <cursor-style>hand</cursor-style>
              <positioning type="absolute">35,18</positioning>
            </texture>

            <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
              u="61" v="0" region-width="21" region-height="23" id="spell_favorite_4">
              <cursor-style>hand</cursor-style>
              <positioning type="absolute">20,32</positioning>
            </texture>

          </children>
        </flow-layout>

        <collapsible expanded="false" id="list">
          <children>
            <grid-layout rows="4" columns="4">
              <children>
                <!-- ROW ONE -->
                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="61" v="0" region-width="21" region-height="23" row="0" column="0"
                  id="spell_0_0">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="82" v="0" region-width="21" region-height="23" row="0" column="1"
                  id="spell_0_1">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="103" v="0" region-width="21" region-height="23" row="0" column="2"
                  id="spell_0_2">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="124" v="0" region-width="21" region-height="23" row="0" column="3"
                  id="spell_0_3">
                  <cursor-style>hand</cursor-style>
                </texture>

                <!-- ROW TWO -->
                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="61" v="23" region-width="21" region-height="23" row="1" column="0"
                  id="spell_1_0">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="82" v="23" region-width="21" region-height="23" row="1" column="1"
                  id="spell_1_1">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="103" v="23" region-width="21" region-height="23" row="1" column="2"
                  id="spell_1_2">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="124" v="23" region-width="21" region-height="23" row="1" column="3"
                  id="spell_1_3">
                  <cursor-style>hand</cursor-style>
                </texture>

                <!-- ROW THREE -->
                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="61" v="46" region-width="21" region-height="23" row="2" column="0"
                  id="spell_2_0">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="82" v="46" region-width="21" region-height="23" row="2" column="1"
                  id="spell_2_1">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="103" v="46" region-width="21" region-height="23" row="2" column="2"
                  id="spell_2_2">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="124" v="46" region-width="21" region-height="23" row="2" column="3"
                  id="spell_2_3">
                  <cursor-style>hand</cursor-style>
                </texture>

                <!-- ROW FOUR -->
                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="61" v="69" region-width="21" region-height="23" row="3" column="0"
                  id="spell_3_0">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="82" v="69" region-width="21" region-height="23" row="3" column="1"
                  id="spell_3_1">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="103" v="69" region-width="21" region-height="23" row="3" column="2"
                  id="spell_3_2">
                  <cursor-style>hand</cursor-style>
                </texture>

                <texture texture="crawling-mysteries:textures/gui/screen/spell_menu_screen.png"
                  u="124" v="69" region-width="21" region-height="23" row="3" column="3"
                  id="spell_3_3">
                  <cursor-style>hand</cursor-style>
                </texture>

              </children>
              <margins>
                <right>10</right>
                <bottom>10</bottom>
              </margins>
            </grid-layout>
          </children>


          <surface>
            <vanilla-translucent></vanilla-translucent>
          </surface>
          <positioning type="relative">100,50</positioning>

        </collapsible>

      </children>

      <horizontal-alignment>center</horizontal-alignment>
      <vertical-alignment>center</vertical-alignment>

      <surface>
        <outline>dark-purple</outline>
      </surface>

    </flow-layout>

  </components>
</owo-ui>